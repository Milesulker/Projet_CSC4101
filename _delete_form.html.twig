<form method="post" action="{{ path('app_galerie_delete', {'id': galerie.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette galerie ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ galerie.id) }}">
    <button class="btn btn-primary">Supprimer</button>
</form>
